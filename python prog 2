class Solution(object):
    def generate(self,ind,subset,ans, candidates,target):
        if target<0:
            return 
        if (ind==len(candidates)):
            return 
        if target==0:
            ans.append(list(subset ))
            return 
        subset.append(candidates[ind])
        self.generate(ind,subset,ans, candidates,target-candidates[ind])#take part
        subset.pop()
        self.generate(ind+1,subset,ans, candidates, target )#not take part
                      
    def combinationSum(self, candidates, target):
        ind=0
        ans=[]
        subset=[]
        self.generate(ind,subset,ans,candidates,target)
        return ans
